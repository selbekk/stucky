{
  "name": "stucky",
  "version": "1.0.2",
  "description": "A sticky table header utility that just works",
  "main": "dist/stucky.js",
  "dependencies": {
    "object-assign": "^4.0.1",
    "raf": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "js-dev-server": "^0.1.1",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/stucky.js -o dist/stucky.js",
    "postbuild": "uglifyjs dist/stucky.js > dist/stucky.min.js && cp src/stucky.css dist/stucky.css",
    "prestart": "npm run build && browserify src/demo.js -t babelify -o dist/demo.js",
    "start": "js-dev-server",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selbekk/stucky"
  },
  "keywords": [
    "sticky",
    "table",
    "header",
    "vanilla",
    "browser",
    "pyah"
  ],
  "author": "Kristofer Selbekk <kristofer@selbekk.io>",
  "license": "ISC"
}
